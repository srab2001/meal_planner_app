{
  "rewrites": [
    {
      "source": "/api/fitness-interview/questions",
      "destination": "/test-api/questions.json"
    },
    {
      "source": "/api/fitness-interview/submit",
      "destination": "/test-api/submit.json"
    },
    {
      "source": "/api/fitness-interview/generate-plan",
      "destination": "/test-api/generate.json"
    },
    {
      "source": "/api/:path*",
      "destination": "https://meal-planner-app-mve2.onrender.com/api/:path*"
    },
    {
      "source": "/auth/:path*",
      "destination": "https://meal-planner-app-mve2.onrender.com/auth/:path*"
    }
  ],
  "headers": [
    {
      "source": "/index.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate, public"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Forwarded-Host",
          "value": "meal-planner-app-chi.vercel.app"
        }
      ]
    }
  ]
}
