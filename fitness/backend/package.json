{
  "name": "@meal_planner/fitness-backend",
  "version": "0.1.0",
  "description": "Fitness module backend - REST API for workout logging, progress tracking, goal management",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development nodemon src/server.js",
    "start": "NODE_ENV=production node src/server.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "migrate": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "migrate:reset": "prisma migrate reset --skip-generate --skip-seed",
    "generate": "prisma generate",
    "seed": "node scripts/seed-exercises.js",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "studio": "prisma studio"
  },
  "keywords": [
    "fitness",
    "workouts",
    "exercise",
    "goal-tracking",
    "progress"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.8.0",
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.1.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.6",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prisma": "^5.8.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
